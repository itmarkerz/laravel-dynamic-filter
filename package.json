{
    "name": "itmarkerz/laravel-dynamic-filter",
    "description": "A Laravel package for applying dynamic query filters",
    "type": "module",
    "license": "MIT",
    "authors": [
        {
            "name": "Binu Mathew",
            "email": "binumathew@itmarkerz.co.in"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Itmarkerz\\LaravelDynamicFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Itmarkerz\\LaravelDynamicFilter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Itmarkerz\\LaravelDynamicFilter\\Providers\\LaravelDynamicFilterServiceProvider"
            ]
        }
    }
}